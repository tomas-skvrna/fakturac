<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %} | Fakturáč</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('bootstrap-css') }}
            {{ encore_entry_link_tags('app-css') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}
        {% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('bootstrap-js') }}
            {{ encore_entry_script_tags('app-js') }}
        {% endblock %}
    </body>
</html>
